{
  "name": "universal-react-webpack-boilerplate",
  "version": "0.0.2",
  "description": "Universal React Webpack Boilerplate",
  "main": "build/server.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src webpack config scripts",
    "coverage": "jest --coverage",
    "cleanup": "shx rm -rf ./build && shx mkdir -p ./build/public",
    "build": "npm run cleanup && npm run webpack:build",
    "dev": "node scripts/run start",
    "start": "cross-env NODE_ENV=production node build/server",
    "webpack:build": "npm run webpack:build:client && npm run webpack:build:server",
    "webpack:build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.client.config.js",
    "webpack:build:server": "cross-env NODE_ENV=production TARGET=SERVER webpack --config webpack/webpack.server.config.js"
  },
  "author": "Martin Broder <hello@martinbroder.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-promise-middleware": "^3.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-class-constructor-call": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-preset-es2015-native-modules": "^6.9.2",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.13.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "jest-cli": "^13.2.3",
    "json-loader": "^0.5.4",
    "jsonp": "^0.2.0",
    "kneden": "^1.0.5",
    "postcss-loader": "^0.9.1",
    "react-a11y": "^0.3.3",
    "react-geomicons": "^2.0.5",
    "react-transform-hmr": "^1.0.4",
    "rebass": "^0.3.0",
    "reflexbox": "^1.1.4",
    "shelljs": "^0.7.1",
    "shx": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.20",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-merge": "^0.14.0",
    "webpack-middleware": "^1.5.1",
    "webpack-stats-plugin": "^0.1.3"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
